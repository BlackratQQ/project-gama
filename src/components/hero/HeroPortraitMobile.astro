---
import OptimizedImage from '../OptimizedImage.astro';

interface Props {
  class?: string;
}

const { class: className = "" } = Astro.props;
---

<OptimizedImage
  src="/images/hero/Portrait-256w.webp"
  alt="Hero portrait"
  class={`w-64 h-auto object-contain opacity-60 ${className}`}
  width={256}
  height={276}
  loading="eager"
  fetchpriority="high"
  decoding="async"
/>